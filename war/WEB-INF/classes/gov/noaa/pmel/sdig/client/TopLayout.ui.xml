<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	
	.fullpagecontainer {
		width: 100%;
		height: 100%;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:Label>This is a pretty label</g:Label>
			<g:Button ui:field="button"></g:Button>
			<g:ListBox ui:field="myList"></g:ListBox>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 