<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:mdep="urn:import:gov.noaa.pmel.sdig.client.panels"
             xmlns:mde="urn:import:gov.noaa.pmel.sdig.client.widgets">
    <g:HTMLPanel>
        <!--

012 pCO2A: Standardization technique description
013 pCO2A: Frequency of standardization

041 pCO2A: Location of seawater intake
030 pCO2A: Depth of seawater intake
059 pCO2A: Water flow rate (L/min)
058 pCO2A: Vented or not

044 pCO2A: Manufacturer of the gas detector
046 pCO2A: Model of the gas detector
049 pCO2A: Resolution of the gas detector
057 pCO2A: Uncertainty of the gas detector

036 pCO2A: Headspace gas flow rate (L/min)
043 pCO2A: Manufacturer of standard gas
028 pCO2A: Concentrations of standard gas
056 pCO2A: Uncertainties of standard gas

060 pCO2A: Water vapor correction method
052 pCO2A: Temperature correction method
031 pCO2A: Drying method for CO2 gas
024 pCO2A: At what temperature was pCO2 reported?

033 pCO2A: Equilibrator type
034 pCO2A: Equilibrator volume (L)
039 pCO2A: How was temperature inside the equilibrator measured?
038 pCO2A: How was pressure inside the equilibrator measured?

        -->
        <b:Form ui:field="form">
            <b:Heading ui:field="heading" size="H3" text="Enter the Information for the CO2 Variables."></b:Heading>
            <b:Heading color="#B22222" size="H5" text="(*) Denotes a required field."></b:Heading>
            <b:FieldSet>
                <b:Container width="100%" pull="LEFT">
                    <b:Row>
                        <b:Column size="SM_12">
                            <b:FormLabel addStyleNames="large_text" text="Add entries for the CO2 variables in the dataset" ></b:FormLabel>
                            <b:gwt.CellTable ui:field="variablesTable" addStyleNames="co2table" visible="false"
                                             bordered="true" condensed="true" striped="true" hover="true"/>
                        </b:Column>
                    </b:Row>
                    <b:Heading size="H4" text="CO2 Common Attributes"/>
                    <!--
                    <b:Row>
                        <b:Column size="SM_12">
                            <b:Pagination ui:field="variablePagination" addStyleNames="tightenTBmargins" visible="false" />
                        </b:Column>
                    </b:Row>
                    -->
                    <mdep:Co2CommonVariablePanel addStyleNames="expand_common" ui:field="common"></mdep:Co2CommonVariablePanel>

                    <b:Row>
                        <b:Column size="SM_12 MD_8 LG_9">
                            <b:FormGroup ui:field="standardizationTechniqueForm"><!-- Calibration method -->
                                <b:FormLabel for="standardizationTechnique" text="Calibration Method"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalStandardizationTechnique" title="24.25 Please describe the calibration procedure." />
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalStandardizationTechnique" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="standardizationTechnique" allowBlank="true" placeholder="Calibration method" b:id="standardizationTechnique"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 MD_4 LG_3">
                            <b:FormGroup ui:field="freqencyOfStandardizationForm"><!-- Frequency of calibration -->
                                <b:FormLabel for="freqencyOfStandardization" text="Frequency of Calibration"></b:FormLabel>
                                <b:Modal title="24.26 How frequently was the calibration carried out, e.g., every 6 hours, after every 10 samples, etc." closable="true" b:id="modalFreqencyOfStandardization"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalFreqencyOfStandardization" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="freqencyOfStandardization" allowBlank="true" placeholder="Frequency of calibration" b:id="freqencyOfStandardization"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>
                    <b:Row ui:field="standardGasRow0">
                        <b:Column size="SM_4">
                            <b:FormGroup ui:field="standardGasManufactureForm0">
                                <b:FormLabel for="standardGasManufacture0" text="Manufacturer of Standard Gas"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalStandardGasManufacture" title="24.27 Manufacturer of the CO2 standard gas."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalStandardGasManufacture" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="standardGasManufacture0" allowBlank="true" placeholder="Manufacturer of standard gas" b:id="standardGasManufacture0"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_2">
                            <b:FormGroup ui:field="gasConcentrationForm0">
                                <b:FormLabel for="standardGasConcentration0" text="Concentration"></b:FormLabel>
                                <b:Modal title="24.28 Concentrations of the CO2 standard gases that were used to calibrate the CO2 sensor, e.g., 200, 350, 510ppm." closable="true" b:id="modalGasConcentration"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalGasConcentration" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="standardGasConcentration0" allowBlank="true" placeholder="Concentration" b:id="standardGasConcentration0"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_2">
                            <b:FormGroup ui:field="standardGasUncertaintiesForm0">
                                <b:FormLabel for="standardGasUncertainty" text="Uncertainty"></b:FormLabel>
                                <b:Modal title="24.29 Uncertainty of the CO2 standard gas, e.g., 0.5%." closable="true" b:id="modalStandardGasUncertainty"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalStandardGasUncertainty" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="standardGasUncertainty0" allowBlank="true" placeholder="Uncertainty" b:id="standardGasUncertainty0"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_2">
                            <b:FormGroup ui:field="standardGasTraceabilityForm">
                                <b:FormLabel for="standardGasTraceability" text="WMO Traceability"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalStandardGasTraceability" title="Traceability of Gas to WMO Standards."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalStandardGasTraceability" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="standardGasTraceability0" allowBlank="true" placeholder="Traceability" b:id="standardGasTraceability0"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_2">
                            <b:FormGroup>
                                <b:FormLabel color="#00000000" for="addStdGasButton" text="*"></b:FormLabel>
                                <b:Modal closable="true" ui:field="stdGasBtnPopover" b:id="stdGasBtnPopover" title="Add another Standard Gas."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#stdGasBtnPopover" dataToggle="MODAL" pull="RIGHT" icon="QUESTION_CIRCLE"></b:Button>
                                <b:Button ui:field="addStdGasButton" pull="RIGHT" type="PRIMARY" text="ADD STD GAS" b:id="addStdGasButton"/>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>
                    <b:Row>
                        <b:Column size="SM_12 MD_6 LG_3">
                            <b:FormGroup ui:field="gasDetectorManufactureForm">
                                <!-- Keep -->
                                <b:FormLabel for="gasDetectorManufacture" text="Gas Detector Manufacturer"></b:FormLabel>
                                <b:Modal title="24.21 Manufacturer of the CO2 sensor." closable="true" b:id="modalGasDetectorManufacture"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalGasDetectorManufacture" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="gasDetectorManufacture" allowBlank="true" placeholder="Manufacturer" b:id="gasDetectorManufacture"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 MD_6 LG_3">
                            <b:FormGroup ui:field="gasDetectorModelForm">
                                <!-- Keep -->
                                <b:FormLabel for="gasDetectorModel" text="Gas Detector Model "></b:FormLabel>
                                <b:Modal title="24.22 Model and number of the CO2 sensor." closable="true" b:id="modalGasDetectorModel"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalGasDetectorModel" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="gasDetectorModel" allowBlank="true" placeholder="Model" b:id="gasDetectorModel"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 MD_6 LG_3">
                            <b:FormGroup ui:field="gasDectectorResolutionForm">
                                <!-- Keep -->
                                <b:FormLabel for="gasDectectorResolution" text="Gas Detector Resolution "></b:FormLabel>
                                <b:Modal title="24.23 Resolution of the CO2 sensor." closable="true" b:id="modalGasDectectorResolution"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalGasDectectorResolution" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="gasDectectorResolution" allowBlank="true" placeholder="Resolution" b:id="gasDectectorResolution"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 MD_6 LG_3">
                            <b:FormGroup ui:field="gasDectectorUncertaintyForm">
                                <!-- Keep -->
                                <b:FormLabel for="gasDectectorUncertainty" text="Gas Detector Uncertainty"></b:FormLabel>
                                <b:Modal title="24.24 Uncertainty of the CO2 sensor." closable="true" b:id="modalGasDectectorUncertainty"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalGasDectectorUncertainty" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="gasDectectorUncertainty" allowBlank="true" placeholder="Uncertainty" b:id="gasDectectorUncertainty"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>

                    <b:Row>
                        <b:Column size="SM_12 MD_6">
                            <b:FormGroup ui:field="pco2FromXco2MethodForm">
                                <!-- Keep -->
                                <b:FormLabel for="pco2FromXco2Method" text="Method to Calculate pCO2 from xCO2 (reference)"></b:FormLabel>
                                <b:Modal title="SOCAT" closable="true" b:id="modalPco2FromXco2Method"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalPco2FromXco2Method" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="pco2FromXco2Method" allowBlank="true" placeholder="Method to calculate pCO2 from xCO2 (reference)" b:id="pco2FromXco2Method"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 MD_6">
                            <b:FormGroup ui:field="fco2FromPco2MethodForm">
                                <!-- Keep -->
                                <b:FormLabel for="fco2FromPco2Method" text="Method to Calculate fCO2 from pCO2 (reference)"></b:FormLabel>
                                <b:Modal title="SOCAT" closable="true" b:id="modalFco2FromPco2Method"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalFco2FromPco2Method" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="fco2FromPco2Method" allowBlank="true" placeholder="Method to calculate fCO2 from pCO2 (reference)" b:id="fco2FromPco2Method"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>

                    <b:Row>
                        <b:Column size="SM_6 LG_4">
                            <b:FormGroup ui:field="equilibratorTypeForm">
                                <!-- Keep -->
                                <b:FormLabel for="equilibratorType" text="Equilibrator Type"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalEquilibratorTypeForm" title="24.13 Type of the equilibrator for the CO2 measurement."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalEquilibratorTypeForm" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="equilibratorType" allowBlank="true" placeholder="Equilibrator type" b:id="equilibratorType"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_6 LG_4">
                            <b:FormGroup ui:field="equilibratorVolumeForm">
                                <!-- Keep -->
                                <b:FormLabel for="equilibratorVolume" text="Equilibrator Volume (L)"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalEquilibratorVolume" title="24.14 The total volume of the CO2 equilibrator."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalEquilibratorVolume" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="equilibratorVolume" allowBlank="true" placeholder="Equilibrator volume (L)" b:id="equilibratorVolume"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="MD_6 LG_4">
                            <b:FormGroup ui:field="ventedForm">
                                <!-- Keep -->
                                <b:FormLabel for="vented" text="Vented or not"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalVented" title="24.15 Is the equilibrator vented or not?"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalVented" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <mde:ButtonDropDown ui:field="vented"></mde:ButtonDropDown>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>

                    <b:Row>
                        <b:Column size="MD_6 LG_4">
                            <b:FormGroup ui:field="flowRateForm">
                                <!-- Keep -->
                                <b:FormLabel for="flowRate" text="Water Flow Rate (L/min)"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalFlowRate" title="24.16 Flow rate of the flow through seawater."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalFlowRate" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="flowRate" allowBlank="true" placeholder="Water flow rate (L/min)" b:id="flowRate"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="MD_6 LG_4" offset="LG_4">
                            <b:FormGroup ui:field="gasFlowRateForm">
                                <!-- Keep -->
                                <b:FormLabel for="gasFlowRate" text="Headspace Gas Flow Rate (L/min)"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalGasFlowRate" title="24.17 Flow rate of the gas from the equilibrator to the CO2 analyzer."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalGasFlowRate" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="gasFlowRate" allowBlank="true" placeholder="Headspace gas flow rate (L/min)" b:id="gasFlowRate"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>

                    <b:Row>
                        <b:Column size="SM_12 MD_6">
                            <b:FormGroup ui:field="equilibratorTemperatureMeasureMethodForm">
                                <!-- Keep -->
                                <b:FormLabel for="equilibratorTemperatureMeasureMethod" text="How was temperature inside the equilibrator measured?"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalEquilibratorTemperatureMeasureMethod"  title="24.18	Please specify whether the temperature inside the equilibrator was measured or not. If so, please describe how the temperature was measured."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalEquilibratorTemperatureMeasureMethod" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="equilibratorTemperatureMeasureMethod" allowBlank="true" placeholder="How was temperature inside the equilibrator measured?" b:id="equilibratorTemperatureMeasureMethod"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 MD_6">
                            <b:FormGroup ui:field="equilibratorTemperatureMeasureUncertaintyForm">
                                <!-- Keep -->
                                <b:FormLabel for="equilibratorTemperatureMeasureUncertainty" text="Uncertainty of Temperature Measured Inside the Equlibrator"></b:FormLabel>
                                <b:Modal title="SOCAT" closable="true" b:id="modalEquilibratorTemperatureMeasureUncertainty"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalEquilibratorTemperatureMeasureUncertainty" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="equilibratorTemperatureMeasureUncertainty" allowBlank="true" placeholder="Uncertainty of temperature measured inside the equlibrator" b:id="equilibratorTemperatureMeasureUncertainty"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>

                    <b:Row>
                        <b:Column size="SM_12 MD_12 LG_4">
                            <b:FormGroup ui:field="equilibratorTemperatureSensorCalibrationMethodForm">
                                <!-- Keep -->
                                <b:FormLabel for="equilibratorTemperatureSensorCalibrationMethod" text="Calibration Method and Frequency for Temperature Sensor(s)"></b:FormLabel>
                                <b:Modal title="SOCAT" closable="true" b:id="modalEquilibratorTemperatureSensorCalibrationMethod"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalEquilibratorTemperatureSensorCalibrationMethod" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="equilibratorTemperatureSensorCalibrationMethod" allowBlank="true" placeholder="Calibration method and frequency for temperature sensor inside the equlibrator" b:id="equilibratorTemperatureSensorCalibrationMethod"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 MD_6 LG_4">
                            <b:FormGroup ui:field="temperatureCorrectionMethodForm">
                                <!-- Keep -->
                                <b:FormLabel for="temperatureCorrectionMethod" text="Temperature Correction Method"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalTemperatureCorrectionMethod" title="24.31 How the temperature effect was corrected."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalTemperatureCorrectionMethod" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="temperatureCorrectionMethod" allowBlank="true" placeholder="Temperature correction method" b:id="temperatureCorrectionMethod"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 MD_6 LG_4">
                            <b:FormGroup ui:field="pco2TemperatureForm">
                                <!-- Keep -->
                                <b:FormLabel for="pco2Temperature" text="At what temperature was fCO2 reported?"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalpco2Temperature" title="24.32 The input could be a constant temperature value, or something like in-situ temperature, temperature of analysis, etc."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalpco2Temperature" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="pco2Temperature" allowBlank="true" placeholder="At what temperature was fCO2 reported?" b:id="pco2Temperature"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>

                    <b:Row>
                        <b:Column size="SM_12 MD_6">
                            <b:FormGroup ui:field="equilibratorPressureMeasureMethodForm">
                                <!-- Keep -->
                                <b:FormLabel for="equilibratorPressureMeasureMethod" text="How was pressure inside the equilibrator measured?"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalEquilibratorPressureMeasureMethod" title="24.19 Please specify whether the pressure inside the equilibrator was measured or not. If so, please describe how the pressure was measured."/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalEquilibratorPressureMeasureMethod" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="equilibratorPressureMeasureMethod" allowBlank="true" placeholder="How was pressure inside the equilibrator measured?" b:id="equilibratorPressureMeasureMethod"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 MD_6">
                            <b:FormGroup ui:field="totalMeasurementPressureDeterminedForm">
                                <!-- Keep -->
                                <b:FormLabel for="totalMeasurementPressureDetermined" text="How was the total measurement pressure determined?"></b:FormLabel>
                                <b:Modal title="SOCAT" closable="true" b:id="modaltotalMeasurementPressureDetermined"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modaltotalMeasurementPressureDetermined" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="totalMeasurementPressureDetermined" allowBlank="true" placeholder="How was pressure inside the equilibrator measured?" b:id="totalMeasurementPressureDetermined"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>

                    <b:Row>
                        <b:Column size="SM_12 LG_6">
                            <b:FormGroup ui:field="totalMeasurementPressureUncertaintyCalculatedForm">
                                <!-- Keep -->
                                <b:FormLabel for="totalMeasurementPressureUncertaintyCalculated" text="Uncertainty of Total Measurement Pressure, and how was this calculated?"></b:FormLabel>
                                <b:Modal title="SOCAT" closable="true" b:id="modaltotalMeasurementPressureUncertaintyCalculated"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modaltotalMeasurementPressureUncertaintyCalculated" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="totalMeasurementPressureUncertaintyCalculated" allowBlank="true" placeholder="Uncertainty of total measurement pressure, and how was this calculated?" b:id="totalMeasurementPressureUncertaintyCalculated"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 LG_6">
                            <b:FormGroup ui:field="calibrationMethodPressureSensorFrequencyForm">
                                <!-- Remove>
                                <b:FormLabel for="calibrationMethodPressureSensorFrequency" text="Calibration method and frequency for pressure sensor(s)"></b:FormLabel>
                                <b:Modal title="SOCAT" closable="true" b:id="modalCalibrationMethodPressureSensorFrequency"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalCalibrationMethodPressureSensorFrequency" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="calibrationMethodPressureSensorFrequency" allowBlank="true" placeholder="Calibration method and frequency for pressure sensor(s)" b:id="calibrationMethodPressureSensorFrequency"></b:TextBox>
                                -->

                                <!-- Keep -->
                                <b:FormLabel for="equilibratorPressureSensorCalibrationMethod" text="Calibration Method and Frequency for Pressure Sensor(s)"></b:FormLabel>
                                <b:Modal title="SOCAT" closable="true" b:id="modalequilibratorPressureSensorCalibrationMethod"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalequilibratorPressureSensorCalibrationMethod" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="equilibratorPressureSensorCalibrationMethod" allowBlank="true" placeholder="Calibration method and frequency for pressure sensor(s)" b:id="calibrationMethodPressureSensorFrequency"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                    </b:Row>

                    <b:Row>
                        <b:Column size="SM_12 MD_6 LG_4">
                            <b:FormGroup ui:field="vaporCorrectionForm">
                                <!-- Keep -->
                                <b:FormLabel for="vaporCorrection" text="Water Vapor Correction Method"></b:FormLabel>
                                <b:Modal closable="true" b:id="modalVaporCorrection" title="24.30 How the water vapor pressure inside the equilibrator corrected?"/>
                                <b:Button tabIndex="999" addStyleNames="benone" dataTarget="#modalVaporCorrection" dataToggle="MODAL" iconSize="NONE" icon="QUESTION_CIRCLE" pull="RIGHT" type="DEFAULT"></b:Button>
                                <b:TextBox ui:field="vaporCorrection" allowBlank="true" placeholder="Water vapor correction method" b:id="vaporCorrection"></b:TextBox>
                            </b:FormGroup>
                        </b:Column>
                        <b:Column size="SM_12 LG_8">
                            <b:FormGroup ui:field="dryingMethodForm">
